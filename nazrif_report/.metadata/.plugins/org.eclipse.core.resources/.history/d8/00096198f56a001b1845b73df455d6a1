	<queryString>
		<![CDATA[select 
	ROW_NUMBER() Over (Order by a.sl) sl,
	a.emp_id,
    b.name,
    c.designation,
    d.department_name,
    a.inventory_details,
    a.qty,
    b.join_date,
    b.ladger_page,
    b.phone,
    b.email,
    a.remarks,
    '' sign

from 
	rcu_inventory_allocation a,
    rcu_employee b,
    rcu_designation c,
    rcu_department d
where 
	b.designation=c.sl and
    d.sl=b.department
    ]]>
	</queryString>
